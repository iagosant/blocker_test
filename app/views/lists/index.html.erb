<p id="notice"><%= notice %></p>

<h1>Listing Lists</h1>

<% @lists.each do |list| %>
  <div class="index_row clearfix">
    <h4 class="list_title"><%= link_to list.name, list %></h4>
    <p class="list_sub_title"><%= list.description %></p>
  </div>
<% end %>

<div class="links">
  <%= link_to "New List", new_list_path %>
</div>
